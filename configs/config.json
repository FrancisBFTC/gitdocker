{
    "RULES" : {
        "max_major": "100",
        "max_minor": "20",
        "max_patch": "none"
    },
    "INIT" : {
        "ignore": [".exe", ".obj", ".o", ".dll"]
    },
    "EXTENSIONS":{
        "lang":[
            {
                "exts": [".c", ".cpp", ".h", ".js"],
                "comments": ["//", "/*", "*/"]
            },
            {
                "exts": [".asm", ".inc"],
                "comments": [";"]
            },
            {
                "exts": [".html"],
                "comments": ["<!--", "-->"]
            }
        ]
    }
}
